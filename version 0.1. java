
import java.util.Scanner;
public class Main
{
    public static void main(String[] args) {
        System.out.println("Enter a value ;");
        Scanner scan= new Scanner(System.in);
        int value= scan. nextInt();
        scan.close();
        int rev=0;
        while(value!=0){
            int rem= value%10;
            rev= rev*10 + rem;
            value= value/10;
        }
        System.out.println(rev);
    }
}
